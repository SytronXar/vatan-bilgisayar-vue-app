A,KAAA,CAAA,GAAA;AAAA,OAAA;AAFA,KAAA;AAIA,GAlBA;AAmBA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,aAAA,KAAA,OAAA,CAAA,IAAA,CAAA,WAAA,GAAA,OAAA,CAAA,KAAA,EAAA,GAAA,IAAA,OAAA;AACA,KAHA;AAIA,IAAA,WAJA,uBAIA,KAJA,EAIA;AACA,UAAA,GAAA,GAAA,CAAA,KAAA,GAAA,CAAA,EAAA,OAAA,CAAA,CAAA,EAAA,OAAA,CAAA,GAAA,EAAA,GAAA,CAAA;AACA,aAAA,GAAA,CAAA,QAAA,GAAA,OAAA,CAAA,uBAAA,EAAA,GAAA,CAAA;AACA;AAPA;AAnBA,CAAA","sourcesContent":["<script>\r\n// @ is an alias to /src\r\nimport Products from \"@/Products\";\r\nexport default {\r\n  name: \"UrunSayfasi\",\r\n  computed: {},\r\n  props: {\r\n    CartId: {\r\n      type: String,\r\n      required: true\r\n    },\r\n    pId: {\r\n      type: String,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      cartItem: Products.inCart.find(inCart => inCart.id === this.CartId),\r\n      product: Products.data.find(data => data.id === this.pId)\r\n    };\r\n  },\r\n  methods: {\r\n    ProductHref() {\r\n      return this.product.name.toLowerCase().replace(/\\s/g, \"-\") + \".html\";\r\n    },\r\n    formatPrice(value) {\r\n      let val = (value / 1).toFixed(0).replace(\".\", \",\");\r\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\r\n    }\r\n  }\r\n};\r\n</script>\r\n<template>\r\n  <router-link\r\n    :id=\"pId\"\r\n    :to=\"{\r\n      name: 'ProductPage',\r\n      params: {\r\n        productId: pId,\r\n        producthref: ProductHref()\r\n      }\r\n    }\"\r\n  >\r\n    <picture class=\"active\">\r\n      <img\r\n        :src=\"product.images[0]\"\r\n        class=\"img-responsive product-list__image\"\r\n      />\r\n    </picture>\r\n    <div class=\"wrapper-product-text\">\r\n      <span class=\"product-name\">{{ product.name }}</span>\r\n      <span class=\"product-cost\">{{ formatPrice(product.cost*cartItem.count) }}</span>\r\n    </div>\r\n  </router-link>\r\n</template>"],"sourceRoot":"src/components/CartButton"}]}